{
  "name": "vite-sustainable",
  "version": "1.0.0",
  "description": "A plugin that generates import maps for your dependencies, so they can be cached across origins",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "author": "Ludwig Schubert (@UtopianContrib)",
  "license": "MIT",
  "dependencies": {
    "vite": "^7.1.9"
  },
  "devDependencies": {
    "@types/node": "^22.10.4",
    "tsc": "^2.0.4",
    "typescript": "^5.9.3"
  }
}
